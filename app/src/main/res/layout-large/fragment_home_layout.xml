                android:orientation="vertical">

                <Button
                    android:id="@+id/button_home_two"
                    android:layout_width="18dp"
                    android:layout_height="0dp"
                    android:layout_gravity="right"
                    android:layout_margin="6dp"
                    android:layout_weight="0.3"
